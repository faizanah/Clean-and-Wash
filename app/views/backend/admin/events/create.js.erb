<% if flash[:errors].present? %>
    <% flash[:errors].each do |e| %>
        flash_error("<%= e %>");
    <% end %>
<% elsif flash[:success].present? %>
    <% @calendar_events.each do |event| %>
        $('#calendar').fullCalendar('renderEvent', $.parseJSON("<%=j render('backend/admin/events/event' , event: event).html_safe %>"));
    <% end %>
    flash_success("<%= flash[:success] %>");
    $(".modal").modal("hide");
<% end %>